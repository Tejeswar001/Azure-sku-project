				"[concat(variables('factoryId'), '/datasets/customer')]",
				"[concat(variables('factoryId'), '/datasets/products')]",
				"[concat(variables('factoryId'), '/datasets/items')]",
				"[concat(variables('factoryId'), '/datasets/DelimitedText1')]"
			]
		}
